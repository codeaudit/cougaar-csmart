<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE EVENTLIST [

<!ELEMENT EVENTLIST ((CYBER|KINETIC)*)>

<!ELEMENT CYBER (PARAM*)>
<!ATTLIST CYBER  TARGET CDATA #REQUIRED
		 TIME CDATA #REQUIRED
		 TYPE CDATA #REQUIRED>

<!ELEMENT KINETIC (PARAM*)>
<!ATTLIST KINETIC LATITUDE CDATA #REQUIRED
  		  LONGITUDE CDATA #REQUIRED
		  TIME CDATA #REQUIRED
		  TYPE CDATA #REQUIRED>

<!ELEMENT PARAM EMPTY>
<!ATTLIST PARAM NAME CDATA #REQUIRED
		VALUE CDATA #REQUIRED>

]>

<EVENTLIST>
	<KINETIC LATITUDE="26.0" LONGITUDE="75.0" TIME="1500000" TYPE="Bomb blast">
		<PARAM NAME="Intensity" VALUE="1"></PARAM>
		<PARAM NAME="Duration"  VALUE="0"></PARAM>
	</KINETIC>
</EVENTLIST>

